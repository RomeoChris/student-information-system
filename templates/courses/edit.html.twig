{% extends 'base.html.twig' %}

{% block content %}

<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
        <div class="card-box">
            <h4 class="header-title m-t-0 text-center">{{ formHeader }}</h4>
            {{ form_start(form) }}
            {{ form_widget(form) }}
            <button type="submit" class="btn btn-info">Update</button>
            {{ form_end(form) }}
        </div>
    </div>
    <div class="col-md-3"></div>
</div>

{% endblock %}
